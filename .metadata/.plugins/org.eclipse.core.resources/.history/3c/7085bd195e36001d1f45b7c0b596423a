package basic;
class Member{
	int id;
	String name;
	String address;
	Member(int id,String name,String address){
		this.id=id;
		this.name=name;
		this.address=address;
	}
}
public class Encapsulation {
	public static void main(String[] args) {
		Member m = new Member(1, "c", "ahmedabad");
		Member m = new Member(2, "c++", "mumbai");
	}
}
