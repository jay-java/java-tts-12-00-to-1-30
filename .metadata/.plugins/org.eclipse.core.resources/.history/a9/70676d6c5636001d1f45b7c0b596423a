package basic;

class Employee{
	int id;
	String name;
	double salary;
	public void show() {
		System.out.println("id : "+id+" name : "+name+" salary : "+salary);
	}
}
public class ConstructorDemo {
	public static void main(String[] args) {
		Employee e1 = new Employee();
		e1.id=1;
		e1.name="java";
		e1.salary=23333.3;
		
	}
}
