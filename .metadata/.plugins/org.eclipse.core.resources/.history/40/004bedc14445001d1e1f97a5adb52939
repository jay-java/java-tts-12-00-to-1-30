package basic;

class Dataa{
	int id;
	String name;
	double sal;
	public Dataa(int id, String name, double sal) {
		super();
		this.id = id;
		this.name = name;
		this.sal = sal;
	}
	@Override
	public String toString() {
		return "Dataa [id=" + id + ", name=" + name + ", sal=" + sal + "]";
	}
	
}

public class ListPractical {
	public static void main(String[] args) {
		
	}
}
