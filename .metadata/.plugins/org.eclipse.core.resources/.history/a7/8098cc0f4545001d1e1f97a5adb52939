package basic;

import java.util.ArrayList;
import java.util.List;

class Dataa{
	int id;
	String name;
	double sal;
	public Dataa(int id, String name, double sal) {
		super();
		this.id = id;
		this.name = name;
		this.sal = sal;
	}
	public String toString() {
		return "Dataa [id=" + id + ", name=" + name + ", sal=" + sal + "]";
	}
	
}

public class ListPractical {
	public static void main(String[] args) {
		Dataa d1 = new Dataa(1, "jeel", 11000);
		Dataa d2 = new Dataa(2, "rudra", 12000);
		Dataa d3 = new Dataa(3, "riken", 13000);
		List<Dataa> list = new ArrayList<Dataa>();
		list.add(d1);
		list.add(d2);
		list.add(d3);
	}
}
